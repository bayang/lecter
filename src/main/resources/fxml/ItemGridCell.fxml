<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXRippler?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="cellWrapper" styleClass="gridCell" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <JFXRippler>
            <VBox>
               <children>
                              <HBox fx:id="firstLine" alignment="CENTER_LEFT">
                                 <children>
                                    <ImageView fx:id="icon" pickOnBounds="true" preserveRatio="true" />
                                    <Label fx:id="fromLabel" styleClass="feed-from" text="Label" wrapText="true">
                                       <HBox.margin>
                                          <Insets left="10.0" />
                                       </HBox.margin>
                                    </Label>
                                 </children>
                              </HBox>
                              <Label fx:id="dateLabel" alignment="BOTTOM_RIGHT" styleClass="date-label" text="Label">
      </Label>
                              <Label fx:id="subjectLabel" styleClass="feed-subject" text="Label" wrapText="true" />
                              <Label fx:id="contentLabel" styleClass="feed-content" text="Label" wrapText="true">
         </Label>
               </children>
            </VBox>
      </JFXRippler>
               </children>
            </VBox>
